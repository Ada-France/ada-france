NAME=adafr
LIBNAME=lib

PLUGINS=

include rules.mk

# Model generation arguments with Dynamo
# --package XXX.XXX.Models db uml/xxx.zargo
DYNAMO_ARGS=db

package:
	rm -rf $(distdir)
	$(DYNAMO) dist $(distdir) package.xml
	tar czf $(DIST_FILE) $(distdir)

post-build:
ifneq (${PLUGINS},)
	for i in  ; do \
	   $(MAKE) -C plugins/$$i ; \
	done
endif

post-generate:
ifneq (${PLUGINS},)
	for i in  ; do \
	   $(MAKE) -C plugins/$$i generate ; \
	done
endif
